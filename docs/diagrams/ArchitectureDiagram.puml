@startuml
!include <office/Users/user>
hide circle
hide member



Package "BrokeMan"<<Rectangle>>{
    class Ui
    class Command
    class Parser
    class Save
    class EntryList
    class Common
}

class "<$user>" as User

User <-> Ui
Ui -> Parser
Parser --> Command
Command --> EntryList
Command -right-> Save
Save <-> Data
Save -left> EntryList
EntryList -> Ui
EntryList -left[hidden]> Common


@enduml